<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.kerawa.app.Health_services"
    android:background="#ffffff">

    <TextView android:text="@string/heath_services" android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView23"
        android:padding="8dp"
        android:background="#eeeeee"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="15dp"
        android:textColor="#222222" />
    <View
        android:layout_width="fill_parent"
        android:layout_height="2dp"
        android:background="@color/blue_kerawa"
        android:layout_below="@+id/textView23"
        android:id="@+id/linearLayout10" />
    <ListView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/health_menu"
        android:layout_below="@+id/linearLayout10"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="2dp"
        android:dividerHeight="1dp"
        android:divider="#888888" />

</RelativeLayout>
